{
  "name": "namui",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Viet Nam Nguyen",
  "homepage": "https://github.com/namvnngu/namui",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/namvnngu/namui"
  },
  "bugs": {
    "url": "https://github.com/namvnngu/namui/issues"
  },
  "scripts": {
    "lint": "biome lint ./",
    "format": "biome format --write ./",
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r --parallel run build",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "./scripts/clean.sh",
    "storybook": "./scripts/storybook.sh",
    "package:count": "./scripts/package-count.sh"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@vitest/ui": "^2.0.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.9.0"
}
